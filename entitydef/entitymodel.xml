    <entity entity-name="PersonEntity" package-name="org.apache.ofbiz.ofbizDemo" title="Person Entity">
        <field name="partyId" type="id"><description>primary sequenced ID</description></field>
        <field name="roleTypeId" type="id"></field>
        <field name="contactMechId" type="id"></field>
        <field name="firstName" type="name"></field>
        <field name="lastName" type="name"></field>
        <field name="emailAddress" type="email"></field>
        <field name="contactNumber" type="tel-number"></field>
        <field name="address" type="name"></field>
        <field name="comments" type="comment"></field>
        <prim-key field="partyId"/>
        <relation type="one" fk-name="PERSON_OD_TYPE_ID" rel-entity-name="Party">
            <key-map field-name="roleTypeId"/>
        </relation>
        <relation type="many" fk-name="PERSONE_OD_TYPE_ID" rel-entity-name="ContactMech">
            <key-map field-name="emailAddress" rel-field-name="contactMechId"/>
        </relation>
        <relation type="many" fk-name="PERSON1_OD_TYPE_ID" rel-entity-name="TelecomNumber">
         <key-map field-name="contactNumber" rel-field-name="contactMechId"/>
        </relation>
        <relation type="many" fk-name="PERSON2_OD_TYPE_ID" rel-entity-name="PostalAddress">
        <key-map field-name="address" rel-field-name="contactMechId"/>
        </relation>
    </entity>

</entitymodel>